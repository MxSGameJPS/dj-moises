{
    "version": 2,
    "builds": [
      {
        "src": "./src/mocks/Festas.json",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "./src/mocks/Festas.json",
        "dest": "./src/mocks/Festas.json"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }